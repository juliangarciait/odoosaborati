            </xpath>
            <!--<xpath expr="//form/sheet/notebook/page[@name='operations']/field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="attributes">
                <attribute name="domain">[('id', 'in', select_product_ids)]</attribute>
            </xpath>-->
        </field>
    </record>

</odoo>